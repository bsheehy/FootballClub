@using Club.Domain.Artifacts
@using Mallon.Core.Web.Models
@using Mallon.Core.Web.Models.HtmlExtensions
@using Club.Services.Utils
@using Club.Domain.Models
@model ModelCommitteeMinute
@{
  ViewBag.Title = "Create Committee Minute";
  string committeeMinutesControlId = ViewBag.CommitteeMinutesControlId;
  string committeeOid = ViewBag.CommitteeOid;
}
@using (Ajax.BeginForm("CommitteeMinuteAjax", "Committee", new AjaxOptions
{
  HttpMethod = "POST",
  OnFailure = "handleAjaxError",
  OnSuccess = "savedModelCommitteeMinute"
}))
{
  
  @Html.AntiForgeryToken()
  @Html.ValidationSummary(false)

  @Html.Hidden("CommitteeMinutesControlId", committeeMinutesControlId)
  @Html.Partial("~/Views/Committee/_CommitteeMinute.cshtml", Model)
  <input id="hiddenSubmit" type="submit" value="Save" class="saveButtonRight k-button" />
}




